{
  "name": "corgi-sandbox",
  "version": "0.0.0",
  "description": "A 3D sandbox game with physics and other stuff.\n\nmade by elburg.",
  "main": "index.js",
  "os": [
    "linux"
  ],
  "scripts": {
    "prepack": "echo \"preparing for packing.\" && rm -rfv ./node_modules ./package-lock.json ~/.npm/_logs/* && echo \"done\"",
    "test": "echo \"Tests:\n\t'testLinuxAll'\t--\tRun Teast Mode (Linux All)\" && exit 0",
    "testLinuxAll": "node --experimental-vm-modules --inspect=localhost:9229 -- index.js --teastMode=true --execFor=LinuxAll",
    "execLinuxAll": "node index.js --execFor=LinuxAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polygonnedpotato/corgi-sandbox.git"
  },
  "keywords": [
    "electron",
    "game",
    "sandbox",
    "desktop",
    "3d",
    "sandbox-game"
  ],
  "author": "elburg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/polygonnedpotato/corgi-sandbox/issues"
  },
  "homepage": "https://github.com/polygonnedpotato/corgi-sandbox#readme",
  "dependencies": {
    "electron": "^15.0.0"
  }
}
